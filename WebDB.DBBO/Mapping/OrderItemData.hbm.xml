<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebDB.DBBO" namespace="WebDB.DBBO" xmlns="urn:nhibernate-mapping-2.2">
  <typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
  <class name="OrderItemData" table="OrderItemData">
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="PONo" type ="TrimString">
      <column name="PONo" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Code" type ="TrimString">
      <column name="Code" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Name" type ="TrimString">
      <column name="Name" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Qty" >
      <column name="Qty" sql-type="int" not-null="false" />
    </property>
    <property name="Price">
      <column name="Price" sql-type="decimal" not-null="false" />
    </property>
    <property name="Discount">
      <column name="Discount" sql-type="float" not-null="false" />
    </property>
    <property name="DiscountName" type ="TrimString">
      <column name="DiscountName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Amount">
      <column name="Amount" sql-type="decimal" not-null="false" />
    </property>
    <property name="ItemType" type ="TrimString">
      <column name="ItemType" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="oOrderData" >
      <column name="OrderDataID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="DataStatus" type ="TrimString">
      <column name="DataStatus" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedID" type ="TrimString">
      <column name="CreatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="DATETIME2" not-null="false" />
    </property>
    <property name="UpdatedID" type ="TrimString">
      <column name="UpdatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="UpdatedDate">
      <column name="UpdatedDate" sql-type="DATETIME2" not-null="false" />
    </property>
  </class>
</hibernate-mapping>