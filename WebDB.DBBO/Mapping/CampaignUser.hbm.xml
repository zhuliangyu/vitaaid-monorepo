<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebDB.DBBO" namespace="WebDB.DBBO" xmlns="urn:nhibernate-mapping-2.2">
	<typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
	<class name="CampaignUser" table="CampaignUser" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="ProgramName">
      <column name="ProgramName" sql-type="nvarchar" not-null="false" />
	</property>
	<property name="FirstName">
		<column name="FirstName" sql-type="nvarchar" not-null="false" />
	</property>
	<property name="LastName">
		<column name="LastName" sql-type="nvarchar" not-null="false" />
	</property>
	<property name="Email">
		<column name="Email" sql-type="nvarchar" not-null="false" />
	</property>
		<property name="PrizeItem">
			<column name="PrizeItem" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="DataStatus" type ="TrimString">
			<column name="DataStatus" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="UpdatedID" type ="TrimString">
			<column name="UpdatedID" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="CreatedID" type ="TrimString">
			<column name="CreatedID" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="CreatedDate">
			<column name="CreatedDate" sql-type="datetime" not-null="false" />
		</property>
		<property name="UpdatedDate">
			<column name="UpdatedDate" sql-type="datetime" not-null="false" />
		</property>

	</class>
</hibernate-mapping>