<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="WebDB.DBBO" namespace="WebDB.DBBO" xmlns="urn:nhibernate-mapping-2.2">
	<typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
	<class name="Ingredient" table="Ingredient" lazy="true" >
		<id name="ID" column="ID">
			<generator class="identity" />
		</id>
		<property name="Name" type ="TrimString">
			<column name="Name" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="LabelClaim" type ="TrimString">
			<column name="LabelClaim" sql-type="nvarchar" not-null="false" />
		</property>
    <property name="LabelClaimUS" type ="TrimString">
      <column name="LabelClaimUS" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="AdditionalInfo" type ="TrimString">
			<column name="AdditionalInfo" sql-type="nvarchar" not-null="false" />
		</property>
		<many-to-one name="oProduct">
			<column name="oProduct" sql-type="int" not-null="false" />
		</many-to-one>
		<property name="ProductCode">
			<column name="ProductCode" sql-type="smalldatetime" not-null="false" />
		</property>
		<property name="GroupNo">
			<column name="GroupNo" sql-type="int" not-null="false" />
		</property>
		<property name="RawMaterialCategory" type ="TrimString">
			<column name="RawMaterialCategory" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="Sequence">
			<column name="Sequence" sql-type="int" not-null="false" />
		</property>
		<property name="DataStatus" type ="TrimString">
			<column name="DataStatus" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="UpdatedID" type ="TrimString">
			<column name="UpdatedID" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="CreatedID" type ="TrimString">
			<column name="CreatedID" sql-type="nvarchar" not-null="false" />
		</property>
		<property name="CreatedDate">
			<column name="CreatedDate" sql-type="datetime" not-null="false" />
		</property>
		<property name="UpdatedDate">
			<column name="UpdatedDate" sql-type="datetime" not-null="false" />
		</property>
	</class>
</hibernate-mapping>