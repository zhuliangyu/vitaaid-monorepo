<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MIS.DBBO" namespace="MIS.DBBO" xmlns="urn:nhibernate-mapping-2.2">
  <typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
  <class name="AS2Log" table="AS2Log" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
      <property name="XactCode" type ="TrimString">
          <column name="XactCode" sql-type="nvarchar" not-null="false" />
      </property>
      <property name="XactCtrlNum" type ="TrimString">
          <column name="XactCtrlNum" sql-type="nvarchar" not-null="false" />
      </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="DATETIME2" not-null="false" />
    </property>
      <property name="Note" type ="TrimString">
          <column name="Note" sql-type="nvarchar" not-null="false" />
      </property>
    <property name="FileName" type ="TrimString">
      <column name="FileName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="AS2Type" type ="TrimString">
      <column name="AS2Type" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>