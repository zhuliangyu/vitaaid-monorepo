<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MIS.DBBO" namespace="MIS.DBBO" xmlns="urn:nhibernate-mapping-2.2">
  <typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
  <class name="WishProduct" table="WishProduct">
	<id name="ID" column="ID">
	  <generator class="identity" />
	</id>
    <many-to-one name="oCustomer">
      <column name="oCustomer" sql-type="int" not-null="false" />
    </many-to-one>

    <property name="CustomerCode" type ="TrimString">
	  <column name="CustomerCode" sql-type="nvarchar" not-null="false" />
	</property>

    
    <property name="CustomerName" type ="TrimString">
      <column name="CustomerName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CustomerEmail" type ="TrimString">
      <column name="CustomerEmail" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ProductCode" type ="TrimString">
      <column name="ProductCode" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ProductName" type ="TrimString">
      <column name="ProductName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Qty">
      <column name="Qty" sql-type="int" not-null="false" />
    </property>
    <property name="SendEmailStatus" type ="TrimString">
      <column name="SendEmailStatus" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="SendEmailDate" type="DateTime">
      <column name="SendEmailDate" not-null="false" sql-type="DATETIME2" />
    </property>


    <property name="DataStatus" type ="TrimString">
	  <column name="DataStatus" not-null="false" sql-type="nvarchar" />
	</property>
	<property name="CreatedID" type ="TrimString">
	  <column name="CreatedID" not-null="false" sql-type="nvarchar" />
	</property>
	<property name="CreatedDate" type="DateTime">
	  <column name="CreatedDate" not-null="false" sql-type="DATETIME2" />
	</property>
	<property name="UpdatedID" type ="TrimString">
	  <column name="UpdatedID" not-null="false" sql-type="nvarchar" />
	</property>
	<property name="UpdatedDate" type="DateTime">
	  <column name="UpdatedDate" not-null="false" sql-type="DATETIME2" />
	</property>
  </class>
</hibernate-mapping>