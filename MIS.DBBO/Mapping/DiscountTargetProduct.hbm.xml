<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="MIS.DBBO" namespace="MIS.DBBO" xmlns="urn:nhibernate-mapping-2.2">
  <typedef name="TrimString" class="MyHibernateUtil.TrimString, MyHibernateUtil"/>
  <class name="DiscountTargetProduct" table="DiscountTargetProduct" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="oDiscountProgram">
      <column name="oDiscountProgram" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="DiscountName" type ="TrimString">
      <column name="DiscountName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ProductCode" type ="TrimString">
      <column name="ProductCode" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ProductName" type ="TrimString">
      <column name="ProductName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedID" type ="TrimString">
      <column name="CreatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="DATETIME2" not-null="false" />
    </property>
  </class>
</hibernate-mapping>