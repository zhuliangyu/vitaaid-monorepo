    <property name="SalesReps" type ="TrimString">
      <column name="SalesReps" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="SalesRepAccounts" type ="TrimString">
      <column name="SalesRepAccounts" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>