<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EQRecipeDetail" table="EQRecipeDetail" lazy="true" >
	<id name="ID" column="ID">
	  <generator class="identity" />
	</id>
	<many-to-one name="oEQRecipe">
		<column name="RecipeID" sql-type="int" not-null="false"/>
	</many-to-one>
	<property name="RecipeName" type ="TrimString">
	  <column name="RecipeName" sql-type="nvarchar" not-null="false" />
	</property>
	<many-to-one name="oRecipeParameter">
		<column name="RecipeParameterID" sql-type="int" not-null="false"/>
	</many-to-one>
	  <property name="RecipeParameterName" type ="TrimString">
		  <column name="RecipeParameterName" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="RecipeParameterValue" type ="TrimString">
	  <column name="RecipeParameterValue" sql-type="nvarchar" not-null="false" />
	</property>
	  <property name="DataStatus" type ="TrimString">
		  <column name="DataStatus" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="CreatedID" type ="TrimString">
		  <column name="CreatedID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="CreatedDate">
		  <column name="CreatedDate" sql-type="datetime2" not-null="false" />
	  </property>
	  <property name="UpdatedID" type ="TrimString">
		  <column name="UpdatedID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="UpdatedDate">
		  <column name="UpdatedDate" sql-type="datetime2" not-null="false" />
	  </property>

  </class>
</hibernate-mapping>