<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SPCSheet" table="SPCSheet" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="SheetNo">
      <column name="SheetNo" sql-type="int" not-null="true" />
    </property>
    <many-to-one name="oLot">
      <column name="oLot" sql-type="int" not-null="false" />
    </many-to-one>
    <many-to-one name="oPackageMaterial">
      <column name="oPackageMaterial" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="CapsuleWt">
      <column name="CapsuleWt" sql-type="float" not-null="false" />
    </property>
    <property name="StdRmWt">
      <column name="StdRmWt" sql-type="float" not-null="false" />
    </property>
    <property name="StandardWt">
      <column name="StandardWt" sql-type="float" not-null="false" />
    </property>
    <property name="Speed" type ="TrimString">
      <column name="Speed" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="TargetLSLWt">
      <column name="TargetLSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetUSLWt">
      <column name="TargetUSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetLCLWt">
      <column name="TargetLCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetUCLWt">
      <column name="TargetUCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="VarLSLWt">
      <column name="VarLSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="VarUSLWt">
      <column name="VarUSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="VarLCLWt">
      <column name="VarLCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="VarUCLWt">
      <column name="VarUCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="MinWt">
      <column name="MinWt" sql-type="float" not-null="false" />
    </property>
    <property name="AvgWt">
      <column name="AvgWt" sql-type="float" not-null="false" />
    </property>
    <property name="MaxWt">
      <column name="MaxWt" sql-type="float" not-null="false" />
    </property>
    <property name="AbnormalSpecCnt">
      <column name="AbnormalSpecCnt" sql-type="int" not-null="false" />
    </property>
    <property name="AbnormalCtlCnt">
      <column name="AbnormalCtlCnt" sql-type="int" not-null="false" />
    </property>
    <property name="ExtraInfo" type ="TrimString">
      <column name="ExtraInfo" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="OPID" type ="TrimString">
      <column name="OPID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="SheetType">
      <column name="SheetType" sql-type="int" not-null="false" />
    </property>
    <bag name="SPCValues" table="SPCValue" lazy="false">
      <key column="oSPCSheet"/>
      <one-to-many class="SPCValue"/>
    </bag>
    <property name="TimeStamp">
      <column name="TimeStamp" sql-type="long" not-null="false" />
    </property>
    <!-- Other -->
    <property name="DataStatus" type ="TrimString">
      <column name="DataStatus" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedID" type ="TrimString">
      <column name="CreatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="datetime2" not-null="false" />
    </property>
    <property name="UpdatedID" type ="TrimString">
      <column name="UpdatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="UpdatedDate">
      <column name="UpdatedDate" sql-type="datetime2" not-null="false" />
    </property>
  </class>
</hibernate-mapping>
