<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="FabProcessLog" table="FabProcessLog" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <property name="LogLevel">
      <column name="LogLevel" sql-type="int" not-null="true" />
    </property>
    <property name="Type" type ="TrimString">
      <column name="Type" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="oLot">
      <column name="LotID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="LotNo" type ="TrimString">
      <column name="LotNo" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="OPID" type ="TrimString">
      <column name="OPID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ProductName" type ="TrimString">
      <column name="ProductName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Message" type ="TrimString">
      <column name="Message" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="smalldatetime" not-null="false" />
    </property>
    <property name="CreatedID" type ="TrimString">
      <column name="CreatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="bProcessed">
      <column name="Processed" sql-type="int" not-null="false" />
    </property>
    <property name="ProcessedDate">
      <column name="ProcessedDate" sql-type="smalldatetime" not-null="false" />
    </property>
    <property name="ProcessedID" type ="TrimString">
      <column name="ProcessedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Client" type ="TrimString">
      <column name="Client" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Comment" type ="TrimString">
      <column name="Comment" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Category" type ="TrimString">
      <column name="Category" sql-type="nvarchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>