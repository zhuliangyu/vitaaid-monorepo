<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="RunCard" table="RunCard" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
    <many-to-one name="oLot">
      <column name="LotID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="LotNo" type ="TrimString">
      <column name="LotNo" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="OPID" type ="TrimString">
      <column name="OPID" sql-type="nvarchar" not-null="true" />
    </property>
    <property name="Status" type ="TrimString">
      <column name="Status" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Message" type ="TrimString">
      <column name="Message" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Addendum" type ="TrimString">
      <column name="Addendum" sql-type="nvarchar" not-null="false" />
    </property>
    <many-to-one name="oCapsule">
      <column name="CapsuleID" sql-type="int" not-null="false" />
    </many-to-one>
    <property name="CapsuleWt">
      <column name="CapsuleWt" sql-type="float" not-null="false" />
    </property>
    <property name="StdRMWt">
      <column name="StdRMWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetLSLWt">
      <column name="TargetLSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetUSLWt">
      <column name="TargetUSLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetLCLWt">
      <column name="TargetLCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="TargetUCLWt">
      <column name="TargetUCLWt" sql-type="float" not-null="false" />
    </property>
    <property name="CapsuleSpeed" type ="TrimString">
      <column name="CapsuleSpeed" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Param1" type ="TrimString">
      <column name="Param1" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Param2" type ="TrimString">
      <column name="Param2" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="ExtraInfo">
      <column name="ExtraInfo" sql-type="int" not-null="false" />
    </property>
    <property name="GroupCode" type ="TrimString">
      <column name="GroupCode" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="GroupSize">
      <column name="GroupSize" sql-type="int" not-null="false" />
    </property>
    <property name="TotalWeight">
      <column name="TotalWeight" sql-type="float" not-null="false" />
    </property>
    <property name="StandardWt">
      <column name="StandardWt" sql-type="float" not-null="false" />
    </property>
    <property name="EQName" type ="TrimString">
      <column name="EQName" sql-type="nvarchar" not-null="false" />
    </property>
    <!-- Other -->
    <property name="DataStatus" type ="TrimString">
      <column name="DataStatus" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedID" type ="TrimString">
      <column name="CreatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" sql-type="datetime2" not-null="false" />
    </property>
    <property name="UpdatedID" type ="TrimString">
      <column name="UpdatedID" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="UpdatedDate">
      <column name="UpdatedDate" sql-type="datetime2" not-null="false" />
    </property>
  </class>
</hibernate-mapping>