<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PackagePRNotification" table="PackagePRNotification" lazy="true" >
    <id name="ID" column="ID">
      <generator class="identity" />
    </id>
	  <property name="PackageCode" type ="TrimString">
      <column name="PackageCode" sql-type="nvarchar" not-null="true" />
    </property>
	  <property name="CategoryCode" type ="TrimString">
		  <column name="CategoryCode" sql-type="nvarchar" not-null="true" />
	  </property>
	  <property name="PackageType" type="ePACKAGETYPE">
      <column name="PackageType" sql-type="int" not-null="true" />
    </property>
    <property name="PackageName" type ="TrimString">
      <column name="PackageName" sql-type="nvarchar" not-null="false" />
    </property>
    <property name="Qty">
      <column name="Qty" sql-type="int" not-null="false" />
    </property>
	  <property name="dStockCount">
		  <column name="dStockCount" sql-type="int" not-null="false" />
	  </property>
	  <property name="AskLotInfo" type ="TrimString">
		  <column name="AskLotInfo" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="CreatedID" type ="TrimString">
		  <column name="CreatedID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="CreatedDate">
		  <column name="CreatedDate" sql-type="datetime2" not-null="false" />
	  </property>
	  <property name="NotifyID" type ="TrimString">
		  <column name="NotifyID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="NotifyDate">
		  <column name="NotifyDate" sql-type="datetime2" not-null="false" />
	  </property>
  </class>
</hibernate-mapping>