<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="POCO" namespace="POCO" xmlns="urn:nhibernate-mapping-2.2">
  <class name="PackagePurchaseReq" table="PackagePurchaseReq" lazy="true" >
	<id name="ID" column="ID">
	  <generator class="identity" />
	</id>
	<property name="OrderNo" type ="TrimString">
	  <column name="OrderNo" sql-type="nvarchar" not-null="false" />
	</property>
	<property name="Status" type="ePurchaseStatus">
	  <column name="Status" sql-type="float" not-null="false" />
	</property>
	<property name="PackageType" type="ePACKAGETYPE">
	  <column name="PackageType" sql-type="int" not-null="true" />
	</property>
	<property name="CategoryCode" type ="TrimString">
	  <column name="CategoryCode" sql-type="nvarchar" not-null="false" />
	</property>
	<many-to-one name="PMSpec">
	  <column name="PMSpec" sql-type="int" not-null="false" />
	</many-to-one>
	<property name="SizePerBox">
	  <column name="SizePerBox" sql-type="float" not-null="false"/>
	</property>
	<property name="BuyingBox">
	  <column name="BuyingBox" sql-type="int" not-null="false"/>
	</property>
	<many-to-one name="CostCurrency">
	  <column name="CostCurrency" sql-type="int" not-null="false" />
	</many-to-one>
	<property name="UnitCost">
	  <column name="UnitCost" sql-type="float" not-null="false"/>
	</property>
	<property name="TotalPrice">
	  <column name="TotalPrice" sql-type="float" not-null="false"/>
	</property>
	<many-to-one name="oPO">
	  <column name="oPurchaseOrder" sql-type="int" not-null="false" />
	</many-to-one>
	<property name="CreatedDate">
	  <column name="CreatedDate" sql-type="datetime2" not-null="false" />
	</property>
	<property name="UpdatedDate">
	  <column name="UpdatedDate" sql-type="datetime2" not-null="false" />
	</property>
	<property name="PurchaseDate">
	  <column name="PurchaseDate" sql-type="smalldatetime" not-null="false" />
	</property>
	<property name="ReceivedWeight">
	  <column name="ReceivedWeight" sql-type="float" not-null="false"/>
	</property>
	<property name="ReceivedQty">
	  <column name="ReceivedQty" sql-type="float" not-null="false"/>
	</property>
	  <property name="CreatedID" type ="TrimString">
		  <column name="CreatedID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="UpdatedID" type ="TrimString">
		  <column name="UpdatedID" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="OldOrderNo" type ="TrimString">
		  <column name="OldOrderNo" sql-type="nvarchar" not-null="false" />
	  </property>
	  <property name="DataStatus" type ="TrimString">
		  <column name="DataStatus" sql-type="nvarchar" not-null="false" />
	  </property>
  </class>
</hibernate-mapping>